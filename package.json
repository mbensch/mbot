    "querystring": "^0.2.0",
    "request": "^2.79.0",
    "wordfilter": "^0.2.6"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "bufferutil": "^2.0.1",
    "cls-bluebird": "^2.0.1",
    "rimraf": "^2.6.0",
    "utf-8-validate": "^3.0.1",
    "webpack": "^2.2.1"
