{
  "name": "marcel-slackbot-starter",
  "version": "0.0.1",
  "description": "A starter kit for building a custom slackbot",
  "main": "./src/index.js",
  "engines": {
    "node": "6.7.0"
  },
  "scripts": {
    "build:clean": "rimraf ./bin",
    "build:webpack": "webpack --config ./webpack.config.js --colors --progress",
    "build": "npm run build:clean && npm run build:webpack",
    "start": "node ./bin/bot.js"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "botkit": "^0.5.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "rimraf": "^2.6.0"
  },
  "keywords": [
    "bots",
    "chatbots",
    "slack"
  ],
  "author": "marcel@message.io",
  "license": "MIT"
}
